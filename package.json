{
  "name": "@slergberg/react-hook-use-interval",
  "version": "0.3.2",
  "description": "A very simple wrapper around setInterval for React.",
  "keywords": [
    "hooks",
    "interval",
    "react",
    "reactjs",
    "setinterval"
  ],
  "license": "MIT",
  "author": "Rodrigo Scomação do Nascimento <rodrigo.sc.na@gmail.com> (https://github.com/rodrigoscna)",
  "homepage": "https://github.com/slergberg/react-hook-use-interval",
  "repository": "github:slergberg/react-hook-use-interval",
  "bugs": "https://github.com/slergberg/react-hook-use-interval/issues",
  "main": "./lib/index.js",
  "unpkg": "dist/react-hook-use-interval.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-replace": "^2.3.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.9.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-visualizer": "^4.0.4"
  },
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/react-hook-use-interval.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/react-hook-use-interval.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf coverage dist es lib stats",
    "lint": "eslint src tests",
    "prepare": "npm run clean && npm run build",
    "pretest": "npm run lint",
    "test": "jest"
  }
}
